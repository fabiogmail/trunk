<userDefinedSQL>CREATE OR REPLACE VIEW VIEW_MSC_CN_ATIVO_SMP5&lt;br/&gt;AS &lt;br/&gt;WITH meses_tabela_dado AS&lt;br/&gt;  (SELECT menor_data,&lt;br/&gt;    maior_data,&lt;br/&gt;    MONTHS_BETWEEN(maior_data, menor_data) diferenca_datas&lt;br/&gt;  FROM&lt;br/&gt;    (SELECT MIN(to_date(TO_CHAR(data,&apos;MM/yyyy&apos;),&apos;MM/yyyy&apos;)) menor_data,&lt;br/&gt;      MAX(to_date(TO_CHAR(data,&apos;MM/yyyy&apos;),&apos;MM/yyyy&apos;)) maior_data&lt;br/&gt;    FROM cdrview_SMP5&lt;br/&gt;    )&lt;br/&gt;  ),&lt;br/&gt;  gerador_meses AS&lt;br/&gt;  ( SELECT menor_data AS meses FROM meses_tabela_dado&lt;br/&gt;  UNION ALL&lt;br/&gt;  SELECT add_months(menor_data,level)&lt;br/&gt;  FROM meses_tabela_dado&lt;br/&gt;    CONNECT BY LEVEL &lt;= diferenca_datas&lt;br/&gt;  ),&lt;br/&gt;  msc_cn_tabela AS&lt;br/&gt;  ( &lt;br/&gt;select distinct bilhetador.nome as msc, cn.nome as cn&lt;br/&gt;  from rel_bilhetador_cn rc,&lt;br/&gt;       bilhetador, &lt;br/&gt;       cn,&lt;br/&gt;       TIPO_TECNOLOGIA,&lt;br/&gt;       tecnologia,&lt;br/&gt;       indicador&lt;br/&gt;where rc.ID_BILHETADOR = bilhetador.ID_BILHETADOR&lt;br/&gt;  and rc.ID_CN = cn.ID_CN&lt;br/&gt;  and bilhetador.ID_TECNOLOGIA = tecnologia.id&lt;br/&gt;  and TIPO_TECNOLOGIA.ID_TIPO_TECNOLOGIA = tecnologia.ID_TIPO_TECNOLOGIA&lt;br/&gt;  and indicador.ID_TIPO_TECNOLOGIA = TIPO_TECNOLOGIA.ID_TIPO_TECNOLOGIA&lt;br/&gt;  and indicador.nome in (&apos;SMP3&apos;,&apos;SMP4&apos;,&apos;SMP5&apos;,&apos;SMP7&apos;)&lt;br/&gt;  ),&lt;br/&gt;  msc_cn_x_meses_gerados AS&lt;br/&gt;  ( SELECT * FROM gerador_meses, msc_cn_tabela&lt;br/&gt;  ),&lt;br/&gt;  msc_cn_meses_x_ativos AS&lt;br/&gt;  (SELECT msc,&lt;br/&gt;    cn,&lt;br/&gt;    meses,&lt;br/&gt;    (SELECT MAX(periodo)&lt;br/&gt;    FROM msc_cn_ativo b&lt;br/&gt;    WHERE a.msc                                         = b.msc&lt;br/&gt;    AND a.cn                                            = b.cn&lt;br/&gt;    AND to_date(TO_CHAR(b.periodo,&apos;MM/yyyy&apos;),&apos;MM/yyyy&apos;) &lt;= a.meses&lt;br/&gt;    ) ultimo_estado&lt;br/&gt;  FROM msc_cn_x_meses_gerados a&lt;br/&gt;  ),&lt;br/&gt;  msc_cn_estado_mes AS&lt;br/&gt;  (SELECT a.msc,&lt;br/&gt;    a.cn,&lt;br/&gt;    a.meses periodo,&lt;br/&gt;    NVL(b.status,&apos;F&apos;) status&lt;br/&gt;  FROM msc_cn_meses_x_ativos a,&lt;br/&gt;    msc_cn_ativo b&lt;br/&gt;  WHERE a.msc         = b.msc (+)&lt;br/&gt;  AND a.cn            = b.cn (+)&lt;br/&gt;  AND a.ultimo_estado = b.periodo (+)&lt;br/&gt;  )&lt;br/&gt;SELECT &quot;MSC&quot;,&quot;CN&quot;,&quot;PERIODO&quot;,&quot;STATUS&quot; FROM msc_cn_estado_mes ORDER BY 1,3</userDefinedSQL>
